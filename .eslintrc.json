{
    "extends": ["standard", "prettier", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "env": {
        "mocha": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": ["sql", "jest", "react"],
    "rules": {
        "no-unexpected-multiline": ["warn"],
        "max-len": ["error", { "code": 650 }],
        "no-set-state": "off",
        "react/prop-types": 0,
        "no-unused-expressions": 0,
        "no-return-assign": 0,
        "new-cap": 0
    }
}
